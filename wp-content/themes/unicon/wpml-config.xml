<wpml-config>
	<custom-fields>
		<custom-field action="ignore">minti_layout</custom-field>
		<custom-field action="ignore">minti_header</custom-field>
		<custom-field action="ignore">minti_titlebar</custom-field>
		<custom-field action="ignore">minti_headercolor</custom-field>
		<custom-field action="ignore">minti_headerimage</custom-field>
		<custom-field action="ignore">minti_footerwidgets</custom-field>
		<custom-field action="ignore">minti_footercopyright</custom-field>
		<custom-field action="ignore">minti_sidebar</custom-field>
		<custom-field action="ignore">minti_heading</custom-field>
		<custom-field action="ignore">minti_blogstyle</custom-field>
		<custom-field action="ignore">minti_blogcategories</custom-field>
		<custom-field action="ignore">minti_hideimage</custom-field>
