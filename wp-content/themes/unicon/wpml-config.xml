		<custom-field action="ignore">minti_screenshot</custom-field>
		<custom-field action="ignore">minti_source</custom-field>
		<custom-field action="ignore">minti_embed</custom-field>
		<custom-field action="ignore">minti_bgurl</custom-field>
		<custom-field action="ignore">minti_bgstyle</custom-field>
		<custom-field action="ignore">minti_bgcolor</custom-field>
	</custom-fields>
	<custom-types>
		<custom-type translate="1">portfolio</custom-type>
	</custom-types>
	<taxonomies>
		<taxonomy translate="1">portfolio_filter</taxonomy>
	</taxonomies>
	<admin-texts>
		<key name="minti_data">
			<key name="textarea_topbar" />
			<key name="textarea_copyright" />
			<key name="text_titlebar_woocommerce" />
			<key name="textarea_copyright" />
			<key name="textarea_copyright" />
		</key>
	</admin-texts>
</wpml-config>